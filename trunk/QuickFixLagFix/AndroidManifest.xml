<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.rc.QuickFixLagFix" android:versionCode="28"
	android:versionName="2.2.4">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".Activities.QuickFixLagFix"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".Activities.OptionActivity"
			android:label="@string/app_name">
		</activity>
		<activity android:name=".Activities.KernelListActivity"
			android:label="@string/app_name">
		</activity>
		<activity android:name=".Activities.ProgressViewActivity"
			android:label="@string/app_name">
		</activity>
		<receiver android:name=".Activities.BootTimeActivity">
			<intent-filter>
				<action android:name="android.intent.action.BOOT_COMPLETED">
				</action>
				<category android:name="android.intent.category.HOME">
				</category>
			</intent-filter>
		</receiver>

		<meta-data android:value="a14c81292d10213" android:name="ADMOB_PUBLISHER_ID" />
	</application>

	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.WRITE_SETTINGS" />
	<uses-permission android:name="android.permission.RESTART_PACKAGES" />
	<uses-permission android:name="android.permission.WAKE_LOCK" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
	<uses-permission android:name="android.permission.WRITE_SECURE_SETTINGS" />

	<uses-sdk android:minSdkVersion="7" android:targetSdkVersion="7" />

</manifest> 